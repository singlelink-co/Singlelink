{
  "name": "singlelink-api",
  "version": "2.2.0",
  "private": true,
  "description": "A single link for everything.",
  "main": "dist/app.js",
  "scripts": {
    "copy": "(copyfiles -f \"src/sql/*.sql\" dist/sql)",
    "start": "node dist/app.js",
    "build": "tsc && npm run copy",
    "dev": "ts-node src/app.ts",
    "postversion": "auto-changelog --output ../CHANGELOG.md && git add ../CHANGELOG.md && cd ../editor && npm version patch",
    "test": "jest",
    "test-debug": "jest --runInBand --detectOpenHandles",
    "add-admin": "ts-node scripts/sl-admin-manager.ts admin add",
    "remove-admin": "ts-node scripts/sl-admin-manager.ts admin remove",
    "list-admins": "ts-node scripts/sl-admin-manager.ts admins",
    "export-stats": "ts-node scripts/stats-exporter.ts",
    "user-gdpr-package": "ts-node scripts/sl-admin-manager.ts admin gdpr-package"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neutron-Creative/Singlelink.git"
  },
  "keywords": [
    "singlelink",
    "link",
    "social-media",
    "linktree",
    "linktree-alternative"
  ],
  "author": "Neutron Creative Inc.",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Neutron-Creative/Singlelink/issues"
  },
  "homepage": "https://github.com/Neutron-Creative/Singlelink#readme",
  "dependencies": {
    "aws-sdk": "^2.796.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bufferutil": "^4.0.3",
    "casbin": "^5.4.0",
    "casbin-basic-adapter": "^0.1.0",
    "fastify": "^3.8.0",
    "fastify-cors": "^4.1.0",
    "fastify-favicon": "^3.0.0",
    "fastify-rate-limit": "^4.0.3",
    "googleapis": "^71.0.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "minio": "^7.0.18",
    "mixpanel": "^0.13.0",
    "nc-deeplink": "^1.0.6",
    "node-cron": "^3.0.0",
    "object-hash": "^2.2.0",
    "pageres": "github:neutron-creative/pageres#master",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/minio": "^7.0.7",
    "@types/mixpanel": "^2.14.2",
    "@types/node": "^14.14.8",
    "@types/node-cron": "^2.0.3",
    "@types/object-hash": "^2.1.0",
    "@types/pg": "^7.14.7",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "copyfiles": "^2.4.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.2"
  }
}
